<resources>
	<settings>

		<!-- Application -->
		<setting propertyKey="@Properties.app_use_sound" title="@Strings.use_sound">
			<settingConfig type="boolean"/>
		</setting>
		<setting propertyKey="@Properties.app_use_vibration" title="@Strings.use_vibration">
			<settingConfig type="boolean"/>
		</setting>

		<setting propertyKey="@Properties.app_color_countdown_work" title="@Strings.color_countdown_work">
			<settingConfig type="list">
				<listEntry value="0">Black</listEntry>
				<listEntry value="1">White</listEntry>
				<listEntry value="2">Orange</listEntry>
				<listEntry value="3">Blue</listEntry>
			</settingConfig>
		</setting>


		<!-- WORKOUT #1 -->
		<setting propertyKey="@Properties.w1_divider" title="@Strings.empty">
			<settingConfig type="alphaNumeric" readonly="true" />
		</setting>
		<setting propertyKey="@Properties.w1_title" title="@Strings.workout_title">
			<settingConfig type="alphaNumeric" maxLength="32" />
		</setting>
		<setting propertyKey="@Properties.w1_enabled" title="@Strings.workout_enabled">
			<settingConfig type="boolean" />
		</setting>


	</settings>
</resources>
